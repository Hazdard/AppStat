   "source": [
    "def l1(x,y) :\n",
    "    return(x-y)**2\n",
    "\n",
    "def grad_l1(w,i) :\n",
    "    return 2*X[i]*((w.T)@X[i]-Y[i])\n",
    "\n",
    "print(X[i].shape)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 10,
   "id": "ddc8f64c",
   "metadata": {},
   "outputs": [],
   "source": [
    "def f1(w):\n",
    "    return (np.linalg.norm(X@w-Y)**2)/n\n",
    "\n",
    "def grad_f1(w):\n",
    "    return X.T@((X@w-Y))/n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 14,
   "id": "63d6b28c",
   "metadata": {},
   "outputs": [],
   "source": [
    "opt_w_ols = np.linalg.inv(X.T@X/n)@X.T@Y/n\n",
    "\n",
    "def compute_stoc(T) :\n",
    "    values_stoc = np.zeros(T)\n",
    "    w_stoc=np.zeros(d)\n",
    "    for t in range(T):\n",
    "        values_stoc[t] = f1(w_stoc)\n",
    "        w_stoc -= gamma*grad_l1(w_stoc,np.random.randint(n))\n",
    "    values_stoc -= f1(opt_w_ols)\n",
    "    return values_stoc\n",
    "\n",
    "def compute_ols(T,ratio) :\n",
    "    values = np.zeros(T)\n",
    "    w_ols= np.zeros(d)\n",
    "    for t in range(T):\n",
    "        if (t%ratio)==0 :\n",
    "            w_ols -= gamma*grad_f1(w_ols)\n",
    "        values[t] = f1(w_ols)   \n",
    "    values -= f1(opt_w_ols)   \n",
    "    return values"
   ]
  },
