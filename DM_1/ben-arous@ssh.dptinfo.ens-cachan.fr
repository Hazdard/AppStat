   "source": [
    "import numpy as np\n",
    "import matplotlib.pyplot as plt\n",
    "import matplotlib.image as mpimg\n",
    "import glob\n",
    "\n",
    "import time"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 6,
   "id": "4d9abb90",
   "metadata": {},
   "outputs": [],
   "source": [
    "data_x=[]\n",
    "data_y=[]\n",
    "d=28*28\n",
    "n=6000\n",
    "\n",
    "for lettre in [\"A\", \"B\", \"C\"] :\n",
    "    for image_path in glob.glob(\"./data/train/\"+lettre+\"/*.png\"):\n",
    "        data_x.append(mpimg.imread(image_path))\n",
    "        if lettre==\"A\" :\n",
    "            data_y.append(1)\n",
    "        else :\n",
    "            data_y.append(-1)\n",
    "      \n",
    "X2=np.reshape(data_x,(n,d)) \n",
    "            \n",
    "for k in range(len(data_x)) :\n",
    "    new=[]\n",
    "    for i in range(28) :\n",
    "        for j in range(28) :\n",
    "            new.append(data_x[k][i][j])\n",
    "    data_x[k]=new\n",
    "    \n",
    "    \n",
    "X=np.array(data_x)\n",
    "Y=np.array(data_y)\n",
    "\n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 123,
   "id": "6a1a941d",
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "168.88132 0.011835627949087007\n"
     ]
    }
   ],
   "source": [
    "lamb=0.1\n",
    "mu=0.1\n",
    "\n",
    "spec, _ = np.linalg.eig(X.T@X)\n",
    "spec_H = np.real(spec)/n + lamb \n",
    "L = np.max(spec_H)\n",
    "\n",
    "gamma = 2/(L+mu)\n",
    "\n",
    "gamma2=0.001\n",
    "\n",
    "print(L,gamma)"
   ]
  },
